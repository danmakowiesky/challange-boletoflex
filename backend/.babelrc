{
  "env": {
    "development": {
      "sourceMaps": true,
      "retainLines": true
    },
    "test": {
      "plugins": ["@babel/plugin-transform-modules-commonjs"]
    }
  },
  "presets": [
    "@babel/preset-env"
  ],
  "targets": {
    "node": "current"
  },
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-proposal-optional-chaining"
  ]
}
